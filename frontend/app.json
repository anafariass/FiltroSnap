{
  "expo": {
    "name": "FiltroSnap",
    "slug": "filtrosnap",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTabletMode": true
    },
    "android": {
      "usesCleartextTraffic": true, 
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow FiltroSnap to access your camera"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow FiltroSnap to access your photos",
          "cameraPermission": "Allow FiltroSnap to access your camera"
        }
      ]
    ]
  }
}
